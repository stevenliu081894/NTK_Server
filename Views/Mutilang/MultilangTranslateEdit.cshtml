@using NTKServer.ViewModels.MultiLang;
@model MultiLangCacheEditVm

<h4>編輯</h4>
<hr />
<div class="row">
    <div class="col-md-4">
        <form asp-action="MultilangTranslateEditPost">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            
            
            <div class="form-group">
                @Html.HiddenFor(m => Model.key)
                @Html.HiddenFor(m => Model.lang)
                <label asp-for="translationKey" class="control-label"></label>
                <input asp-for="translationKey" class="form-control" readonly />
                <span asp-validation-for="translationKey" class="text-danger"></span>
            </div>

            <div class="form-group">
                <label asp-for="translation" class="control-label"></label>
                <input asp-for="translation" class="form-control" />
                <span asp-validation-for="translation" class="text-danger"></span>
            </div>
          
            <div class="form-group">
                <input type="submit" value="Save" class="btn btn-primary" />
            </div>
        </form>
    </div>
</div>

<div>
    <a asp-action="MultilangIndex">返回列表</a>
</div>

